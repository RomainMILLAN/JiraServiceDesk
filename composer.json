{
    "name": "romainmillan/jira-service-desk",
    "type": "project",
    "license": "MIT",
    "description": "A jira service desk client application.",
    "version": "0.0.1",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "babdev/pagerfanta-bundle": "^4.5",
        "doctrine/dbal": "^3.9.4",
        "doctrine/doctrine-bundle": "^2.13.2",
        "doctrine/doctrine-migrations-bundle": "^3.4.1",
        "doctrine/orm": "^3.3.2",
        "gedmo/doctrine-extensions": "^3.19",
        "pagerfanta/doctrine-dbal-adapter": "^4.7",
        "pagerfanta/doctrine-orm-adapter": "^4.7",
        "pagerfanta/pagerfanta": "^4.7.1",
        "pagerfanta/twig": "*",
        "runtime/frankenphp-symfony": "^0.2.0",
        "spiriitlabs/form-filter-bundle": ">=11.0.6",
        "stof/doctrine-extensions-bundle": "^1.13",
        "symfony/console": "7.2.*",
        "symfony/dotenv": "7.2.*",
        "symfony/flex": "^2.5",
        "symfony/form": "7.2.*",
        "symfony/framework-bundle": "7.2.*",
        "symfony/messenger": "7.2.*",
        "symfony/options-resolver": "7.2.*",
        "symfony/runtime": "7.2.*",
        "symfony/security-bundle": "7.2.*",
        "symfony/stimulus-bundle": ">=2.23",
        "symfony/translation": "7.2.*",
        "symfony/twig-bundle": "7.2.*",
        "symfony/ux-twig-component": "^2.23",
        "symfony/validator": "7.2.*",
        "symfony/webpack-encore-bundle": "^2.2",
        "symfony/yaml": "7.2.*",
        "twig/extra-bundle": "^2.12|^3.20",
        "twig/twig": "^2.12|^3.20"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "bump-after-update": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.2.*",
            "docker": true
        }
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^4.0",
        "symfony/maker-bundle": "^1.62.1",
        "symfony/stopwatch": "7.2.*",
        "symfony/web-profiler-bundle": "7.2.*"
    }
}
