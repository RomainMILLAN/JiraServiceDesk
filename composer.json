{
    "name": "romainmillan/jira-service-desk",
    "type": "project",
    "license": "MIT",
    "description": "A jira service desk client application.",
    "version": "0.3.0",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "babdev/pagerfanta-bundle": "^4.5",
        "cweagans/composer-patches": "^1.7.3",
        "doctrine/dbal": "^3.9.4",
        "doctrine/doctrine-bundle": "^2.14.0",
        "doctrine/doctrine-migrations-bundle": "^3.4.2",
        "doctrine/orm": "^3.3.3",
        "gedmo/doctrine-extensions": "^3.20",
        "lesstif/jira-cloud-restapi": "^1.8",
        "pagerfanta/doctrine-dbal-adapter": "^4.7.1",
        "pagerfanta/doctrine-orm-adapter": "^4.7.1",
        "pagerfanta/pagerfanta": "^2.0.1",
        "pagerfanta/twig": ">=4.7.1",
        "runtime/frankenphp-symfony": "^0.2.0",
        "spiriitlabs/form-filter-bundle": ">=11.0.6",
        "stof/doctrine-extensions-bundle": "^1.14",
        "symfony/console": "7.2.*",
        "symfony/doctrine-messenger": "7.2.*",
        "symfony/dotenv": "7.2.*",
        "symfony/flex": "^2.5.1",
        "symfony/form": "7.2.*",
        "symfony/framework-bundle": "7.2.*",
        "symfony/http-client": "7.2.*",
        "symfony/mailer": "7.2.*",
        "symfony/messenger": "7.2.*",
        "symfony/monolog-bundle": "^3.10",
        "symfony/options-resolver": "7.2.*",
        "symfony/runtime": "7.2.*",
        "symfony/security-bundle": "7.2.*",
        "symfony/stimulus-bundle": ">=2.24",
        "symfony/translation": "7.2.*",
        "symfony/twig-bundle": "7.2.*",
        "symfony/ux-autocomplete": "^2.24",
        "symfony/ux-twig-component": "^2.24",
        "symfony/validator": "7.2.*",
        "symfony/webhook": "7.2.*",
        "symfony/webpack-encore-bundle": "^2.2",
        "symfony/yaml": "7.2.*",
        "symfonycasts/reset-password-bundle": "^1.23.1",
        "twig/extra-bundle": "^2.12|^3.21",
        "twig/string-extra": "^3.21",
        "twig/twig": "^2.12|^3.21.1",
        "vlucas/phpdotenv": "^5.6.2"
    },
    "config": {
        "allow-plugins": {
            "cweagans/composer-patches": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "bump-after-update": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.2.*",
            "docker": true
        },
        "patches": {
            "lesstif/jira-cloud-restapi": {
                "Issue #91: update search() to work with search/jql": "https://github.com/lesstif/php-JiraCloud-RESTAPI/pull/97.patch"
            }
        }
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.7.1",
        "symfony/debug-bundle": "7.2.*",
        "symfony/maker-bundle": "^1.63.0",
        "symfony/stopwatch": "7.2.*",
        "symfony/web-profiler-bundle": "7.2.*",
        "symplify/coding-standard": "^12.3.3",
        "symplify/easy-coding-standard": "^12.5.16",
        "slevomat/coding-standard": "^8.18.0"
    }
}
