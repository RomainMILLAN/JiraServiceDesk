@font-face {
    font-family: 'Nunito';
    src: url('/admin/fonts/Nunito-Light.eot');
    src: local('Nunito Light'), local('Nunito-Light'), url('/admin/fonts/Nunito-Light.eot?#iefix') format('embedded-opentype'), url('/admin/fonts/Nunito-Light.woff') format('woff'), url('/admin/fonts/Nunito-Light.ttf') format('truetype'), url('/admin/fonts/Nunito-Light.svg#Roboto') format('svg');
    font-weight: 300;
    font-style: normal;
}

